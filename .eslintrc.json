{
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "browser": true,
        "jest": true
    },
    "rules": {
        "no-use-before-define": "off",
        "class-methods-use-this": "off",
        "no-param-reassign": "off",
        "no-restricted-syntax": "off",
        "prefer-destructuring": "off", //как можно переписать строчку 74 app.js чтобы это правило прошло?
        "import/extensions": [
        "error",
            {
                "js": "ignorePackages"
            }
        ],
        "import/no-extraneous-dependencies": [
            "error", 
                { 
                    "devDependencies": true 
                }
            ]
    }
}